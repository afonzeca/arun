{
  "name": "afonzeca/arun",
  "description": "Arun CLI Microframework for Php 7.2+",
  "keywords": [
    "microframework",
    "framework",
    "arun",
    "arun-core",
    "cli"
  ],
  "type": "project",
  "time": "2018-11-06",
  "license": "Apache-2.0",
  "homepage": "https://github.com/afonzeca/arun",
  "support": {
    "issues": "https://github.com/afonzeca/arun/issues",
    "source": "https://github.com/afonzeca/arun"
  },
  "authors": [
    {
      "name": "Angelo Fonzeca",
